{
    "expo": {
        "name": "prayer-app",
        "slug": "prayer-app",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/WISE_app_icon.png",
        "splash": {
            "image": "./assets/WISE_app_icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#ebecf0"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "infoPlist": {
                "UIBackgroundModes": [
                    "fetch"
                ]
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/WISE_app_icon.png",
                "backgroundColor": "#ebecf0"
            }
        },
        "owner": "wise-mobile-app"
    }
}